{
  "name": "wjhm-nextjs",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "d": "yarn codegen:develop",
    "dev": "yarn codegen:develop",
    "develop": "run-p storybook:start next:develop",
    "deploy": "yarn && yarn codegen && yarn codegen:fix && yarn build && yarn export",
    "export": "next export",
    "format": "yarn prettier",
    "format-check": "yarn prettier --list-different",
    "icons": "svgr --no-dimensions --ext tsx -d src/components/atoms/icons src/assets/images/icons",
    "prettier": "prettier \"**/*.{css,js,json,jsx,md,mdx,scss,ts,tsx}\"",
    "lint": "yarn linter",
    "lint-fix": "yarn linter --fix",
    "linter": "eslint . --color",
    "serve": "next start -p 4000",
    "start": "yarn develop",
    "storybook": "yarn storybook:start",
    "test": "start-server-and-test dev 3000 cypress:dev",
    "test-cli": "start-server-and-test dev 3000 cypress:run",
    "validate": "yarn format-check && yarn lint",
    "validate-fix": "yarn format && yarn lint-fix && yarn validate",
    "upgrade": "npm-check --update",
    "codegen:develop": "run-s codegen codegen:fix develop",
    "codegen:fix": "node src/node/generation/fixSchema.ts",
    "cypress:dev": "cypress open --config baseUrl=http://localhost:3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "next:develop": "next dev",
    "netlify:upload": "netlify deploy -p",
    "storybook:assets": "node src/node/storybook/assets.ts",
    "storybook:build": "build-storybook  -c src/.storybook -o storybook-public --quiet",
    "storybook:deploy": "run-s storybook:build storybook:assets netlify:upload",
    "storybook:start": "start-storybook -c src/.storybook -p 6006 -s public --quiet"
  },
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "@svgr/webpack": "^5.4.0",
    "dotenv": "^8.2.0",
    "formik": "^2.1.5",
    "framer-motion": "^2.6.15",
    "graphql": "^15.1.0",
    "graphql-request": "^3.0.0",
    "he": "^1.2.0",
    "html-react-parser": "^0.14.0",
    "html5-device-mockups": "3.2.1",
    "intersection-observer": "^0.11.0",
    "isomorphic-unfetch": "^3.0.0",
    "mailtoui": "^1.0.3",
    "moment": "^2.29.0",
    "next": "^9.5.2",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.17.1",
    "react": "16.13.1",
    "react-breakpoints": "3.0.3",
    "react-dom": "16.13.1",
    "react-grid-system": "^7.1.1",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.27.1",
    "react-lazy-load": "^3.1.13",
    "react-slick": "^0.27.10",
    "react-transition-group": "^4.4.1",
    "react-twitter-embed": "^3.0.3",
    "react-youtube": "^7.12.0",
    "sanitize-html-react": "^1.13.0",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.4.5",
    "smooth-scroll": "^16.1.3",
    "styled-components": "^5.1.1",
    "swr": "^0.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.7",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@storybook/addon-actions": "^6.0.20",
    "@storybook/addon-docs": "^6.0.20",
    "@storybook/addon-knobs": "^6.0.20",
    "@storybook/addon-links": "^6.0.20",
    "@storybook/addon-viewport": "^6.0.20",
    "@storybook/addons": "^6.0.20",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.20",
    "@svgr/cli": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/cypress": "^5.0.2",
    "@types/node": "^14.6.1",
    "@types/react": "^16.9.48",
    "@types/react-dom": "16.9.8",
    "@types/react-youtube": "^7.6.2",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^5.0.0",
    "cypress-dark": "^1.7.14",
    "cypress-wait-until": "^1.7.1",
    "eslint": "^7.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^5.1.4",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.1.0",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "netlify-cli": "^2.59.0",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "replace-in-file": "^6.1.0",
    "start-server-and-test": "^1.11.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^4.0.2"
  }
}
